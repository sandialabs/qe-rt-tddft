&control
    prefix = 'graphene_proton_2x2'
    calculation = 'scf'
    restart_mode = 'from_scratch'
    pseudo_dir = '../pseudo/'
    outdir = './scratch/'
    tstress = .false.
    tprnfor = .true.
/
&system
    ibrav = 12
    a = 9.84
    b = 9.84
    c = 20.0
    cosab = -0.5
    nat = 9
    ntyp = 2
    ecutwfc = 100.0
    ecutrho = 400.0
    occupations = 'smearing'
    smearing = 'gaussian'
    degauss = 0.01
/
&electrons
    diagonalization = 'davidson'
    conv_thr = 1.d-6
/
ATOMIC_SPECIES
H    1   H.upf
C   12   C.upf


